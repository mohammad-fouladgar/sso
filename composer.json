{
  "name": "fouladgar/sso",
  "description": "",
  "keywords": [
  ],
  "support": {
    "issues": "https://github.com/mohammad-fouladgar/sso/issues",
    "source": "https://github.com/mohammad-fouladgar/sso"
  },
  "authors": [
    {
      "name": "Mohammad Fouladgar",
      "email": "fouladgar.dev@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "symfony/routing": "^5.1",
    "symfony/http-foundation": "^5.1",
    "symfony/dependency-injection": "^5.1",
    "symfony/yaml": "^5.1",
    "symfony/config": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "mockery/mockery": "^1.4",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Fouladgar\\SSO\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fouladgar\\SSO\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --coverage-text",
    "test-coverage": "vendor/bin/php-coveralls -v"
  },
  "config": {
    "discard-changes": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
